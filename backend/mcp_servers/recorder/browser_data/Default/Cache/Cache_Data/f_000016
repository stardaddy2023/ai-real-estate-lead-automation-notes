			$('<a class="ss-right" id="searchButton" data-role="button" data-inline="true" data-theme="b" data-icon="search" data-iconpos="right" href="/web/searchResults/DOCSEARCH55S10">Search</a>')
				.insertBefore('#SelfService-1766879440695-control-clear')
				.click(function(event) {
				event.preventDefault();
				if(validatePage($('#SelfService-1766879440695-search-form'))) {
					// If any items are selected, show them a warning that their results will
					// disappear if they perform a new search
					var selectedItems = $('ul').find('input.ss-facet-select:checked');
					if (selectedItems.length > 0) {
						selfservice.search.openSelectedItemsWarningPopup('selectedItemsWarningNewSearch');
					} else {
						executeSearch();
					}
				}
			});
		
			$(document).on('keydown', function (event) {
				// If we don't suppress the enter keydown, we get strange behavior and errors
				// Don't prevent defaults if the login window is open as enter should work for login submit not search
				if (((event.which && event.which == 13) || (event.keyCode && event.keyCode == 13)) && $('.ui-panel-open').length < 1) {
					event.preventDefault();
					return false;
				} else if (event.target && event.target.type != undefined && event.target.type.toLowerCase() == 'number') {
					var key = selfservice.determineKeyCodeProperty(event);
					if (!selfservice.userPressedValidKeyForInputNumberTypeBox(key)) {
						event.preventDefault();
					}
				}
			});
		
			selfservice.document.handleTab = function (event) {
				if((selfservice.testvar) && (selfservice.testvar.prop('id') !== $(document.activeElement).prop('id'))) {
					selfservice.testvar = null;
					return; 
				}
				var acItemSelected = $(document).data('Autocomplete');
				$(document).data('Autocomplete', false);
		
				if ($(document).data('listFieldFocus') || $(event.target).closest("li").hasClass("autoCompleteList")) {
					event.stopPropagation();
					event.preventDefault();
					$(document).data('listFieldFocus', false);
		
					// Move focus to the next text field
					var $input = $(document.activeElement);
					var inputs = $input.closest('form').find(':input:visible');
					if (event.shiftKey) {
						var previousInput = inputs.eq(inputs.index($input) - 1);
						previousInput.focus();
					} else {
						var nextInput = inputs.eq(inputs.index($input) + 1);
						if (typeof nextInput.prop('id') == 'undefined') {
							$('#searchButton').focus();
						} else {
							$(nextInput).focus();
						}
					}
					return false;
				}
				return true;
			};
		
			selfservice.document.rebindOnEvents('keyup', function(e) {
				// Don't prevent defaults if the login window is open as enter should work for login submit not search
				var popupOpen = $.mobile.popup.active;
				var panelOpen =  $('.ui-panel-open').length > 0;
		
				if ((e.which && e.which == 9) || (e.keyCode && e.keyCode == 9)) {
					// This is set when the enter key is handled from an autocomplete (name suggest), read it and clear
					return selfservice.document.handleTab(e);
				} else if ((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)) {
					// This is set when the enter key is handled from an autocomplete (name suggest), read it and clear
					var acItemSelected = $(document).data('Autocomplete');
					$(document).data('Autocomplete', false);
		
					if (!acItemSelected && $(e.target).closest("li").hasClass("autoCompleteList")) {
						if($(e.target).val() != ''){
		
							$("ul[id$='aclist']").each(function(){
								$(this).hide()
							});
							$input = $(e.target);
							$input.val('');
						}
					}
					if (acItemSelected) {
						e.preventDefault();
						return false;
					}
		
					if (!panelOpen && !popupOpen && !acItemSelected) {
						if(validatePage($('#SelfService-1766879440695-search-form'))) {
							// If any items are selected, show them a warning that their results will disappear if they perform a new search
							var selectedItems = $('ul').find('input.ss-facet-select:checked');
							if (selectedItems.length > 0) {  
								selfservice.search.openSelectedItemsWarningPopup('selectedItemsWarningNewSearch');
							} else {
								executeSearch();
							}
						}
						e.preventDefault();
						return false;
					} else if (popupOpen) {
						var popup = $('.ui-popup-active').find('[data-role=popup]');
						var back = popup.find('[data-rel=back]');
						if (back && back.length > 0) {
							back[0].click(); // Only call first back button, not all of them
						} else {
							popup.popup('close');
						}
					}
				}
			});
		
			$('<a class="ss-right" id="clearSearchButton" data-role="button" data-inline="true" data-theme="b" data-icon="refresh" data-iconpos="left">Clear Selections</a>')
				.insertBefore('#SelfService-1766879440695-control-clear')
				.click(function(event) {
					event.preventDefault();
					var url = '/web/clearSearchResults/DOCSEARCH55S10';
					$.ajax({
						type: 'POST',
						url: url,
						success: function() {
							$('form[id$=search-form] select').each(function() { 
								$(this).val(''); 
								$(this).selectmenu('refresh');
							});
							location.reload();
						}, 
						error: function(jqXHR, textStatus, errorThrown){
							responseElement.empty();
							responseElement.append('<h2>We&#39;re sorry. An error occurred while clearing search results</h2>');
							responseElement.trigger('create');
						}
					});
				});
				$('#SelfService-1766879440695-search-controls').trigger('create');
			});
		
			var applySearchText = function() {
				var $input = $(document.activeElement);
				var searchText = $input.val();
				if (searchText.length > 0) {
					var $holder = $("#" + $input.prop('id') + "-holder");
					var exists = false;
					$.each($holder.find("li.cblist-input-list"), function(index, value) {
						if($(value).text() === searchText) {
							$(value).css("background","lightblue");
							setTimeout(function() {
								$(value).css("background","initial");
							},500);
							exists = true;
							return false;
						}
					});
				if(!exists) {
					selfservice.search.addSearchItem(searchText, searchText, $holder, $input, "-searchInput");
				}
				$input.val('');
					$("#" + $input.prop('id') + "-aclist").empty().hide();
				}
			};
		
			var responseElement = $('#SelfService-1766879440695-search-results');
			var resultsUrl = '/web/searchResults/DOCSEARCH55S10';
			var filterUrl = '/web/filterSearchResults/DOCSEARCH55S10';
			var buttons = $('#SelfService-1766879440695-search-buttons');
			selfservice.search.initializeSearchObjects(responseElement, resultsUrl, filterUrl, buttons);
		
			var executeSearch = function() {
				var responseElement = $('#SelfService-1766879440695-search-results');
				var resultsUrl = '/web/searchResults/DOCSEARCH55S10';
				var filterUrl = '/web/filterSearchResults/DOCSEARCH55S10';
				var searchForm = $('#SelfService-1766879440695-search-form');
				var controls = $('#SelfService-1766879440695-search-controls');
				var buttons = $('#SelfService-1766879440695-search-buttons');
				
				
				var ae = $(document.activeElement);
				if (ae.prop('id').substring(0, 6) == 'field_') {
					$('#searchButton').focus();
				}
		
				$("ul[id$='aclist']").each(function() {
					$(this).hide()
				});

				selfservice.search.performSearch(responseElement, resultsUrl, filterUrl, searchForm, controls, buttons);		
				selfservice.search.closeSelectedItemsWarningPopup('selectedItemsWarningNewSearch');
			};
			</script>
				</div>
			</li>
		
	
	
