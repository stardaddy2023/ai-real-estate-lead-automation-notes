		$("#SelfService-1766879440695-passwordChangeInternal").trigger("create");
		$("#SelfService-1766879440695-passwordChange-popup").trigger("updatelayout");
		$.mobile.activePage.trigger("pagecreate");
	});
});

$("#SelfService-1766879440695-passwordChange-popup").on("popupafteropen", function() {	
	$("#SelfService-1766879440695-passwordChange-popup").popup("reposition", "positionTo: window");
});

$("#SelfService-1766879440695-passwordChange-popup").on("popupafterclose", function() {
	$("#field_OldPassword").val("");
	$("#field_NewPassword").val("");
	$("#field_ConfirmPassword").val("");
});	
</script>

		
		






