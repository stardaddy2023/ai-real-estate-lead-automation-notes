				$('a.advancedSearch').on('click', function (event) {
					event.preventDefault();
					event.stopPropagation();
					var href=event.target.href;
					var searchField=$(event.target).data('searchField');
					// Store the existing value for later use
					$(event.target).data('searchField', $(searchField).val());
					var searchDoc = $(window).data('searchDoc.' + searchField);
					if (!searchDoc) {
						searchDoc = {'searchField': searchField};
					}
					var advancedSearchBuilt=false;
					$('#SelfService-1766879440695-advancedSearchBuilder-popup').popup({
						beforeposition: function() {
							if (!advancedSearchBuilt) {
								$('.ui-popup-container').css('width','40em');
								$.ajax({
									url: href,
									type: 'POST',
									data: searchDoc,
									dataType: 'html',
									success: function(data) {
										$('#SelfService-1766879440695-advancedSearchBuilderInternal').html(data);
										$('#SelfService-1766879440695-advancedSearchBuilder-popup').trigger('create');
									},
									error: function(jqXHR, textStatus, errorThrown){
										$('.ss-validation-messages').clear();
										$('.ss-validation-messages').append($('<ul><li>We&#39;re sorry. An error occurred opening the dialog</li></ul>'));
									}
								});
								advancedSearchBuilt=true;
							}
						},
						afterclose: function() {
							var searchText = $(window).data('advancedSearchText');
							if (searchText) {
								$('#field_' + searchField).val(searchText);
								$('#field_' + searchField).focus();
								applySearchText();
							}
						}
					}).popup('open', {positionTo: 'window', transition: 'pop'});
				});
		
				var show = $('#field_UseAdvancedSearch').is(':checked');
				showAdvancedSearch(show);
			
		
			
				// create and show popup dialog 
				var recentSearchUrl = '/web/search/recentSearches';
				$('#SelfService-1766879440695-recentSearchPopup').popup({
					beforeposition: function() {
						$('.ui-popup-container').css('width','50em');
						$.ajax({
							type: 'POST',
							url: recentSearchUrl,
							processData: false,
							async: false,
							dataType: 'html',
							success: function(data) {
								$('#SelfService-1766879440695-recentSearchPopupInternal').html(data);
								$('#SelfService-1766879440695-recentSearchPopup').trigger('create');
								selfservice.truncateText($('.recent-search-query-text'), 80);
							}, 
							error: function(jqXHR, textStatus, errorThrown){
								alert('We&#39;re sorry. An error occurred while saving your search');
							}
						});
					},
					afterclose: function() {
						var url = $(window).data('recentSearchUrl');
						if (url) {
							$.ajax({
								type: 'POST',
								url: url,
								processData: false,
								async: false,
