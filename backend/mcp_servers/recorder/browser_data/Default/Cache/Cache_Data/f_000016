		$('#SelfService-1766879440695-page').on('pageinit', function () {
			var sequenceUrl = '';
			var forwardCheck = '';
			var reverseUrl = '';
			var hasPrevious = 'false';
			
			if (forwardCheck.length > 0) {
				$('<a class="ss-right" id="proceedButton" data-role="button" data-inline="true" data-theme="b" data-icon="arrow-r" data-iconpos="right">Next</a>')
					.insertBefore('#SelfService-1766879440695-footer-clear')
					.click(function () {
						selfservice.redirect(sequenceUrl, true);
					});

				if (hasPrevious === 'true') {
					$('<a class="ss-right" id="goBackButton" data-role="button" data-inline="true" data-icon="arrow-l" data-theme="b" data-iconpos="left">Previous</a>')
						.insertBefore('#SelfService-1766879440695-footer-clear')
						.click(function () {
							selfservice.redirect(reverseUrl, true);
						});
				}
			}
			$('#SelfService-1766879440695-footer').trigger('create');
		});

		$('.ss-content').addClass('full-screen');
		$('.ui-grid-a').addClass('full-screen');
		$('.ui-grid-b').addClass('full-screen');
		</script>
