				showAdvancedSearch = function(show) {
					$.each($('a.advancedSearch'), function(index, target) {
						var fieldId = 'field_' + $(target).data('searchField');
						var field = $('#' + fieldId);
						var parent = field.closest('ul');
						var label = $('label[for=' + fieldId + ']');
						var button = $('a.advancedSearch');
						var clearButton =  $('a.ui-input-clear.ui-icon-delete');
		
						if (show) {
							button.fadeIn('fast');
							clearButton.css("right", "20px");
						} else {
							button.hide();
							clearButton.css("right", "0px");
						}
					});
				};
				</script>
		
